{
  "name": "react-color-a11y",
  "version": "0.0.0",
  "description": "React higher-order component to automatically enforce color accessibility",
  "author": "David Lounsbrough",
  "license": "MIT",
  "repository": "Codenough-LLC/react-color-a11y",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "rollup -c",
    "build-watch": "rollup -c --watch",
    "publish-npm": "pnpm clean && pnpm build && npm publish",
    "start-demo": "pnpm --prefix demo start",
    "build-demo": "pnpm --prefix demo build",
    "deploy-demo": "pnpm build-demo && gh-pages -d demo/build",
    "lint": "eslint --ext .ts,.tsx . && pnpm --prefix demo lint",
    "test": "pnpm cypress run --component",
    "test-interactive": "pnpm cypress open"
  },
  "keywords": [
    "color",
    "colors",
    "contrast",
    "accessibility",
    "a11y",
    "npm",
    "package",
    "react",
    "typescript"
  ],
  "files": [
    "/lib"
  ],
  "dependencies": {
    "colord": "^2.9.3",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.15.11",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "cypress": "^12.9.0",
    "eslint": "^8.37.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.20.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "webpack": "^5.77.0"
  }
}
