{
  "name": "react-color-a11y",
  "version": "0.0.2",
  "description": "React higher-order component to automatically enforce color accessibility",
  "author": "David Lounsbrough",
  "license": "MIT",
  "repository": "Codenough-LLC/react-color-a11y",
  "main": "dist/index",
  "typings": "dist/index",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "webpack --mode=production",
    "publish-npm": "pnpm clean && pnpm build && npm publish",
    "start-demo": "pnpm --prefix demo start",
    "build-demo": "pnpm --prefix demo build",
    "deploy-demo": "pnpm build-demo && gh-pages -d demo/build",
    "lint": "eslint --ext .ts,.tsx . && pnpm --prefix demo lint",
    "test": "pnpm cypress run --component",
    "test-interactive": "pnpm cypress open"
  },
  "keywords": [
    "color",
    "colors",
    "contrast",
    "accessibility",
    "a11y",
    "npm",
    "package",
    "react",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "colord": "^2.9.3",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "compression-webpack-plugin": "^10.0.0",
    "cypress": "^12.8.0",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  }
}
